@article{Soummer_2007,
	doi = {10.1086/520913},
	url = {https://doi.org/10.1086/520913},
	year = 2007,
	month = {nov},
	publisher = {American Astronomical Society},
	volume = {669},
	number = {1},
	pages = {642--656},
	author = {Remi Soummer and Andre Ferrari and Claude Aime and Laurent Jolissaint},
	title = {Speckle Noise and Dynamic Range in Coronagraphic Images},
	journal = {The Astrophysical Journal},
	abstract = {This paper is concerned with the theoretical properties of high-contrast coronagraphic images in the context of exoplanet searches. We derive and analyze the statistical properties of the residual starlight in coronagraphic images and describe the effect of a coronagraph on the speckle and photon noise. Current observations with coronagraphic instruments have shown that the main limitations to high-contrast imaging are due to residual quasi-static speckles. We tackle this problem in this paper and propose a generalization of our statistical model to include the description of static, quasi-static, and fast residual atmospheric speckles. The results provide insight into the effects on the dynamic range of wave front control, coronagraphy, active speckle reduction, and differential speckle calibration. The study is focused on ground-based imaging with extreme adaptive optics, but the approach is general enough to be applicable to space, with different parameters.}
}

