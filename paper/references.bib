@article{guyon_adaptive_2017,
	title = {Adaptive {Optics} {Predictive} {Control} with {Empirical} {Orthogonal} {Functions} ({EOFs})},
	url = {http://arxiv.org/abs/1707.00570},
	abstract = {Atmospheric wavefront prediction based on previous wavefront sensor measurements can greatly enhance the performance of adaptive optics systems. We propose an optimal linear approach based on the Empirical Orthogonal Functions (EOF) framework commonly employed for atmospheric predictions. The approach offers increased robustness and significant performance advantages over previously proposed wavefront prediction algorithms. It can be implemented as a linear pattern matching algorithm, which decomposes in real time the input (most recent wavefront sensor measurements) into a linear sum of previously encountered patterns, and uses the coefficients of this linear expansion to predict the future state. The process is robust against evolving conditions, unknown spatio-temporal correlations and non-periodic transient events, and enables multiple sensors (for example accelerometers) to contribute to the wavefront estimation. We illustrate the EOFs advantages through numerical simulations, and demonstrate filter convergence within 1 minute on a 1 kHz rate system. We show that the EOFs approach provides significant gains in high contrast imaging by simultaneously reducing residual speckle halo and producing a residual speckle halo that is spatially and temporally uncorrelated.},
	urldate = {2020-09-22},
	journal = {arXiv:1707.00570 [astro-ph]},
	author = {Guyon, Olivier and Males, Jared},
	month = jul,
	year = {2017},
	note = {arXiv: 1707.00570},
	keywords = {Astrophysics - Instrumentation and Methods for Astrophysics},
	file = {Guyon_Males_2017_Adaptive Optics Predictive Control with Empirical Orthogonal Functions (EOFs).pdf:/Users/miles/Zotero/storage/NITU2X44/Guyon_Males_2017_Adaptive Optics Predictive Control with Empirical Orthogonal Functions (EOFs).pdf:application/pdf;arXiv.org Snapshot:/Users/miles/Zotero/storage/SXBWYG37/1707.html:text/html;Guyon_Males_2017_Adaptive Optics Predictive Control with Empirical Orthogonal Functions (EOFs)_annotated.pdf:/Users/miles/Zotero/storage/6LA4SR92/Guyon_Males_2017_Adaptive Optics Predictive Control with Empirical Orthogonal Functions (EOFs)_annotated.pdf:application/pdf}
}


@inproceedings{guyon_wavefront_2011,
	title = {Wavefront control with the {Subaru} {Coronagraphic} {Extreme} {Adaptive} {Optics} ({SCExAO}) system},
	volume = {8149},
	url = {https://www-spiedigitallibrary-org.eres.library.manoa.hawaii.edu/conference-proceedings-of-spie/8149/814908/Wavefront-control-with-the-Subaru-Coronagraphic-Extreme-Adaptive-Optics-SCExAO/10.1117/12.894293.short},
	doi = {10.1117/12.894293},
	abstract = {The Subaru Coronagraphic Extreme Adaptive Optics (SCExAO) system uses advanced coronagraphic technique for high contrast imaging of exoplanets and disks as close as 1 lambda/D from the host star. In addition to unusual optics, achieving high contrast at this small angular separation requires a wavefront sensing and control architecture which is optimized for exquisite control and calibration of low order aberrations. The SCExAO system was thus designed to include the wavefront sensors required for bias-free high sensitivity and high speed wavefront measurements. Information is combined from two infrared wavefront sensors and a fast visible wavefront sensors to drive a single MEMS type deformable mirror mounted on a tip-tilt mount. The wavefront sensing and control architecture is highly integrated with the coronagraph system.},
	urldate = {2020-09-28},
	booktitle = {Astronomical {Adaptive} {Optics} {Systems} and {Applications} {IV}},
	publisher = {International Society for Optics and Photonics},
	author = {Guyon, Olivier and Martinache, Frantz and Clergeon, Christophe and Russell, Robert and Groff, Tyler and Garrel, Vincent},
	month = sep,
	year = {2011},
	keywords = {\_tablet},
	pages = {814908},
	file = {Guyon et al_2011_Wavefront control with the Subaru Coronagraphic Extreme Adaptive Optics.pdf:/Users/miles/Zotero/storage/JM4ELC22/Guyon et al_2011_Wavefront control with the Subaru Coronagraphic Extreme Adaptive Optics.pdf:application/pdf;Snapshot:/Users/miles/Zotero/storage/V8B8348T/12.894293.html:text/html}
